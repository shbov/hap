{
  "name": "hap-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc && yarn sentry:sourcemaps",
    "start": "node dist/index.js",
    "dev": "nodemon index.ts",
    "lint": "eslint src/**/*.ts",
    "migrate": "npx sequelize-cli db:migrate",
    "test": "jest",
    "format:write": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org faculty-of-computer-science-hs --project hap-api ./dist && sentry-cli sourcemaps upload --org faculty-of-computer-science-hs --project hap-api ./dist"
  },
  "type": "commonjs",
  "keywords": [],
  "author": "",
  "jest": {
    "preset": "ts-jest",
    "verbose": true,
    "clearMocks": true,
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@jest/globals": "^29.7.0",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "jest": "^29.7.0",
    "make-coverage-badge": "^1.2.0",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "sequelize-cli": "^6.6.2",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@sentry/cli": "^2.31.2",
    "@sentry/node": "^8.4.0",
    "@sentry/profiling-node": "^8.4.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.17.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.19",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.0.3",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "react-native-calendar-picker": "^8.0.2",
    "sequelize": "^6.37.1",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3",
    "uuid": "^9.0.1"
  }
}
